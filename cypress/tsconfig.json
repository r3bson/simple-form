{
    "compilerOptions": {
        "baseUrl": "http://localhost:3000/",
        "strict": true,
        "target": "es5",
        "jsx": "preserve",
        "types": ["cypress", "@testing-library/cypress"],
        "lib": ["dom", "esnext"],
